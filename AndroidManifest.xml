<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cyanogenmod.filemanager"
    android:versionName="@string/application_version">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	
    <application
        android:icon="@drawable/filemanager"
        android:label="@string/application_name" android:allowBackup="true" android:hardwareAccelerated="true" android:testOnly="false" android:allowClearUserData="true" android:logo="@drawable/filemanager" android:debuggable="true" android:enabled="true">
        
        <activity
            android:name=".FilemanagerActivity"
            android:label="@string/application_name" >
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
        
    </application>
    
</manifest>
